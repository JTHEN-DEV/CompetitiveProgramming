#include <bits/stdc++.h>

using namespace std;

int main(){
    int t, n, x, y;
    cin >> t;
    for (int i = 0; i < t; i++){
        cin >> x >> y >> n;
        int r;
        for (r = n; r > 0 && r > n-x; r--){
            if (r%x == y){
                break;
            }
        }
        cout << r << endl;
    }
}
